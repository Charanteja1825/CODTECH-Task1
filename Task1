num1 =int(input("Give first number: "))

num2 =int(input("Give second number: "))

operator = input("Give operator:")

if operator == "+":
   print(f"Sum of two numbers is:{num1+num2}")
  
elif operator == "-":
   print(f"Sub of two numbers is:{num1-num2}")
   
elif operator == "*":
   print(f"product of two numbers is:{num1*num2}")
   
elif operator == "/":
   print(f"Div of two numbers is:{num1/num2}")
   
else:
    print("Invalid operator")
